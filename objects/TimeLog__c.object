        <fullName>Revenue__c</fullName>
        <description>Total Revenue generated by this Timelog</description>
        <externalId>false</externalId>
        <formula>IF(No_Charge__c, 0, Duration__c *  Related_Project__r.Billable_Rate__c )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Revenue generated by this timelog</inlineHelpText>
        <label>Revenue</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
