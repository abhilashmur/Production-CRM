    <fields>
        <fullName>Account_Customer__c</fullName>
        <externalId>false</externalId>
        <formula>Related_Project__r.Customer__r.Name</formula>
        <label>Account (Customer)</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Break__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Any unbillable hours spent?</inlineHelpText>
        <label>Break</label>
        <precision>3</precision>
        <required>false</required>
        <scale>1</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Description__c</fullName>
        <description>Description of the TimeLog entry</description>
        <externalId>false</externalId>
        <inlineHelpText>What is this about?</inlineHelpText>
        <label>Description</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Duration__c</fullName>
        <description>Hours spent</description>
        <externalId>false</externalId>
        <formula>ROUND((DATETIMEVALUE(End_Time__c) - DATETIMEVALUE(Start_Time__c)) * 24 - Break__c, 2)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Calculates time spent in hours. End - Start - Break = Duration</inlineHelpText>
        <label>Duration</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>End_Time__c</fullName>
        <description>End time</description>
        <externalId>false</externalId>
        <inlineHelpText>Enter the End time for the TimeLog</inlineHelpText>
        <label>End Time</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
